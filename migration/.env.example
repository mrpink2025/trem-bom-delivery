# Supabase Self-Hosted Configuration Template
# Copy this file to .env and fill in your actual values
# Generated for deliverytrembao.com.br migration

# Studio settings
STUDIO_DEFAULT_ORGANIZATION="Your Organization Name"
STUDIO_DEFAULT_PROJECT="Your Project Name"

# Database settings
POSTGRES_HOST=db
POSTGRES_DB=postgres
POSTGRES_PORT=5432
POSTGRES_PASSWORD=<YOUR_SECURE_DB_PASSWORD_HERE>

# API settings
API_EXTERNAL_URL=https://your-domain.com
SUPABASE_PUBLIC_URL=https://your-domain.com

# Auth settings
SITE_URL=https://your-domain.com
ADDITIONAL_REDIRECT_URLS=https://your-domain.com/**
DISABLE_SIGNUP=false
ENABLE_EMAIL_SIGNUP=true
ENABLE_EMAIL_AUTOCONFIRM=false
ENABLE_PHONE_SIGNUP=true
ENABLE_PHONE_AUTOCONFIRM=false
ENABLE_ANONYMOUS_USERS=false

# JWT settings - GENERATE YOUR OWN SECURE JWT SECRET
JWT_SECRET=<GENERATE_YOUR_OWN_JWT_SECRET_AT_LEAST_32_CHARS>
JWT_EXPIRY=3600

# Email/SMTP settings
SMTP_HOST=your-smtp-host.com
SMTP_PORT=587
SMTP_USER=your-smtp-user
SMTP_PASS=<YOUR_SMTP_PASSWORD_HERE>
SMTP_ADMIN_EMAIL=admin@your-domain.com
SMTP_SENDER_NAME="Your App Name"

# Mailer URL paths
MAILER_URLPATHS_INVITE=/auth/v1/verify
MAILER_URLPATHS_CONFIRMATION=/auth/v1/verify
MAILER_URLPATHS_RECOVERY=/auth/v1/verify
MAILER_URLPATHS_EMAIL_CHANGE=/auth/v1/verify

# API Keys (REPLACE WITH YOUR GENERATED KEYS)
ANON_KEY=<YOUR_ANON_KEY_HERE>
SERVICE_ROLE_KEY=<YOUR_SERVICE_ROLE_KEY_HERE>

# Dashboard credentials
DASHBOARD_USERNAME=supabase
DASHBOARD_PASSWORD=<YOUR_SECURE_DASHBOARD_PASSWORD_HERE>

# Kong ports
KONG_HTTP_PORT=8000
KONG_HTTPS_PORT=8443

# PostgREST
PGRST_DB_SCHEMAS=public,storage,graphql_public

# Logflare
LOGFLARE_API_KEY=<YOUR_LOGFLARE_API_KEY_HERE>

# Image proxy
IMGPROXY_ENABLE_WEBP_DETECTION=true

# Docker socket
DOCKER_SOCKET_LOCATION=/var/run/docker.sock

# Edge Functions Secrets - REPLACE WITH YOUR ACTUAL API KEYS
OPENAI_API_KEY=<YOUR_OPENAI_API_KEY_HERE>
RESEND_API_KEY=<YOUR_RESEND_API_KEY_HERE>
MAPBOX_ACCESS_TOKEN=<YOUR_MAPBOX_TOKEN_HERE>
STRIPE_SECRET_KEY=<YOUR_STRIPE_SECRET_KEY_HERE>